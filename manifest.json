

{
    "manifest_version": 3,
    "name": "Chrome Exetension",
    "version": "1.0",
    "description": "A chrome exetension, which stops you from entering websites like youtube and makes you firs read a wiokibedia article and answer questions about it.",
    "permissions": ["activeTab", "scripting", "tabs", "storage", "alarms"],
    "host_permissions": ["<all_urls>"],
    "action": { "default_popup": "popup.html", "default_icon": { "16": "edit.png"} },
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
  ],

  "web_accessible_resources": [{
    "resources": ["popup.css"],
    "matches": ["<all_urls>"]
  }]

}
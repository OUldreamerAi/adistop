{
    "manifest_version": 3,
    "name": "Adistop",
    "version": "1.0",
    "description": "A chrome extension, which stops you from entering websites like youtube and makes you first read a wikipedia article and answer questions about it.",
    "permissions": ["activeTab", "scripting", "tabs", "storage", "alarms"],
    "host_permissions": [
        "<all_urls>",
        "https://ai.hackclub.com/*",
        "https://en.wikipedia.org/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": { "default_popup": "popup.html" },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["popup.css"],
            "matches": ["<all_urls>"]
        }
    ]
}